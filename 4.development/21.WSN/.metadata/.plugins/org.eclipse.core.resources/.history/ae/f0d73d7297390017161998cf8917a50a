#ifndef microgen_h
#define microgen_h

#define NUM_OF_COMMANDS 4

typedef struct{
    int DCBUS;
    int VPV;
    int IPV;
    int PWR;
}pv2wifi;



const char* pv2wifi_comands[] = {
                                 "#DCBUS#05#",
                                 "#VPV#03#",
                                 "#IPV#03#",
                                 "#PWR#03#"
};



#endif

